#include <iostream>
using namespace std;

int main() {
    int matrix[2][2],sum = 0;

    cout << "enter matrix: " << endl;

    for (int i = 0; i < 2; i++) {
        for (int j = 0; j < 2; j++) {
            cin >> matrix[i][j];
            sum += matrix[i][j];
        }
    }

    cout << "sum matrix= " << sum << endl;

    return 0;
}
