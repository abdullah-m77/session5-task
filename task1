#include <iostream>
using namespace std;

int main() {
    int arr[4];
    cout << "enter 4 number: " << endl;


    for (int i = 0; i < 4; i++) {
        cin >> arr[i];
    }


    int maxNum = arr[0];


    for (int i = 1; i < 4; i++) {
        if (arr[i] > maxNum) {
            maxNum = arr[i];
        }
    }


    cout << "maximum number: " << maxNum << endl;

    return 0;
}
